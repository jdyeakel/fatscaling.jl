Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"Basic", " ", "constants"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"\[CurlyEpsilon]", "=", "0.05"}], ";"}], 
   "                             ", 
   RowBox[{"(*", 
    RowBox[{"acceptable", " ", "ruin", " ", "probability"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{"\[CurlyPhi]", "=", 
     RowBox[{"InverseCDF", "[", 
      RowBox[{
       RowBox[{"NormalDistribution", "[", "]"}], ",", 
       RowBox[{"1", "-", 
        RowBox[{"\[CurlyEpsilon]", "/", "2"}]}]}], "]"}]}], ";"}], "   ", 
   RowBox[{"(*", " ", 
    RowBox[{"\[TildeTilde]", "1.95996"}], "*)"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Full", " ", "ruin", " ", "equation", " ", "f", 
     RowBox[{"(", 
      RowBox[{"x", "|", "a"}], ")"}]}], "*)"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"Clear", "[", "f", "]"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"CDFN", "[", "z_", "]"}], ":=", 
     RowBox[{"CDF", "[", 
      RowBox[{
       RowBox[{"NormalDistribution", "[", "]"}], ",", "z"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", 
      RowBox[{"x_", ",", "a_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "2"}], " ", "a", " ", "x"}], "]"}], " ", 
       RowBox[{"CDFN", "[", 
        RowBox[{"-", 
         RowBox[{"(", 
          RowBox[{"x", "-", "a"}], ")"}]}], "]"}]}], "+", 
      RowBox[{"CDFN", "[", 
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"x", "+", "a"}], ")"}]}], "]"}], "-", "\[CurlyEpsilon]"}]}], 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Exact", " ", "root", " ", "SExact", 
     RowBox[{"(", "a", ")"}], " ", "with", " ", "adaptive", " ", "starting", " ",
      "guess"}], "*)"}], "\n", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SExact", "[", 
     RowBox[{"a_", "?", "NumericQ"}], "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xSeed", ",", "sol"}], "}"}], ",", 
      RowBox[{
       RowBox[{"xSeed", "=", 
        RowBox[{"If", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"a", "<", "\[CurlyPhi]"}], ",", "\[CurlyPhi]", ",", 
          RowBox[{"(*", 
           RowBox[{"diffusion", " ", "seed"}], "*)"}], "\[IndentingNewLine]", 
          
          RowBox[{
           RowBox[{"Log", "[", 
            RowBox[{"1", "/", "\[CurlyEpsilon]"}], "]"}], "/", 
           RowBox[{"(", 
            RowBox[{"2", " ", "a"}], ")"}]}]}], "          ", 
         RowBox[{"(*", 
          RowBox[{"drift", " ", "seed"}], "*)"}], "\[IndentingNewLine]", 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"sol", "=", 
        RowBox[{"Quiet", "@", 
         RowBox[{"FindRoot", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", 
             RowBox[{"x", ",", "a"}], "]"}], "==", "0"}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "xSeed"}], "}"}], ",", 
           RowBox[{"WorkingPrecision", "->", "30"}], ",", 
           RowBox[{"AccuracyGoal", "->", "15"}], ",", 
           RowBox[{"PrecisionGoal", "->", "15"}]}], "\[IndentingNewLine]", 
          "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"MatchQ", "[", 
          RowBox[{"sol", ",", 
           RowBox[{"{", 
            RowBox[{"_", "->", "_"}], "}"}]}], "]"}], ",", 
         RowBox[{"x", "/.", " ", "sol"}], ",", 
         RowBox[{"Missing", "[", "\"\<noRoot\>\"", "]"}]}], 
        "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}], 
   "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Two", " ", "asymptotic", " ", "formulas", " ", 
     RowBox[{"(", 
      RowBox[{"\[Sigma]", " ", 
       RowBox[{"\[Sqrt]", "\[Tau]"}], " ", "normalised", " ", "to", " ", 
       "1"}], ")"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Sdiff", "[", "a_", "]"}], ":=", "\[CurlyPhi]"}], ";"}], 
   "                       ", 
   RowBox[{"(*", 
    RowBox[{"diffusion", " ", "approximation"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Sdrift", "[", "a_", "]"}], ":=", 
     RowBox[{
      RowBox[{"Log", "[", 
       RowBox[{"1", "/", "\[CurlyEpsilon]"}], "]"}], "/", 
      RowBox[{"(", 
       RowBox[{"2", " ", "a"}], ")"}]}]}], ";"}], "          ", 
   RowBox[{"(*", 
    RowBox[{"drift", " ", "approximation"}], "*)"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"::", "Section", "::"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Evaluate", " ", "over", " ", "a", " ", "grid", " ", "of", " ", "a"}], "-",
      "values"}], "*)"}], "\n", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"aVals", "=", 
     RowBox[{"Range", "[", 
      RowBox[{"0.1", ",", "10", ",", "0.1"}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"exact", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Log10", "[", "a", "]"}], ",", 
         RowBox[{"Log10", "[", 
          RowBox[{"SExact", "[", "a", "]"}], "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "aVals"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"diffApp", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Log10", "[", "a", "]"}], ",", 
         RowBox[{"Log10", "[", 
          RowBox[{"Sdiff", "[", "a", "]"}], "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "aVals"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"driftApp", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Log10", "[", "a", "]"}], ",", 
         RowBox[{"Log10", "[", 
          RowBox[{"Sdrift", "[", "a", "]"}], "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"a", ",", "aVals"}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
